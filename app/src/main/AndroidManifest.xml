<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="claudia.rent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".HomeActivity"
            android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseGameActivity"
            android:theme="@style/BarTheme">
        </activity>
        <activity android:name=".EnterScoreActivity"
            android:theme="@style/BarTheme">
        </activity>
        <activity android:name=".HistoryActivity"
            android:theme="@style/BarTheme">
        </activity>
        <activity android:name=".RankingActivity"
            android:theme="@style/BarTheme">
        </activity>
        <activity android:name=".RulesActivity"
            android:theme="@style/BarTheme">
        </activity>
        <activity android:name=".SetupGameActivity"
            android:theme="@style/BarTheme">
        </activity>
    </application>

</manifest>